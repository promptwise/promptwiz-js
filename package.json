{
  "name": "promptwiz",
  "version": "0.1.0",
  "description": "Promptwiz-js is a server-side TypeScript/JavaScript library for prompting large language models (LLMs) using a shared interface compatible with the Promptwise service. `npm install promptwiz-js`",
  "main": "./build/cjs/index.bundle.js",
  "module": "./build/esm/index.js",
  "typings": "./build/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js"
    }
  },
  "scripts": {
    "build:types": "yarn run tsc --project ./tsconfig.json --emitDeclarationOnly",
    "build": "yarn clean && node ./scripts/build_lib.js && yarn build:types",
    "clean": "rm -rf ./build",
    "docs": "node ./scripts/build_docs.js"
  },
  "author": "Aaron Goin",
  "license": "MIT",
  "dependencies": {
    "js-tiktoken": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/esbuild-plugin": "^2.2.0",
    "autoprefixer": "^10.4.12",
    "esbuild": "^0.15.11",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.2.1",
    "postcss": "^8.4.18",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "files": [
    "build"
  ],
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19"
  }
}
